"""Parameters to specifically express the LSTM architecture."""

from enum import Enum, auto


class LSTMParam(Enum):
    """LSTM Architecture Hyperparameters."""

    HIDDEN_DIM = auto()
    OUTPUT_DIM = auto()
    N_LAYERS = auto()
    BATCH_SIZE = auto()
    DROP_PROB = auto()
    LEARNING_RATE = auto()
    SEQUENCE_LENGTH = auto()
