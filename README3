from pathlib import Path
import unicodedata as ud

print("CWD:", Path.cwd())

p = Path("examples/jupyter_notebooks") / "M M.png"
print("Target repr:", repr(str(p)))
print("Parent exists?", p.parent.exists())
print("File exists?", p.exists())

# List what's actually there, with codepoints:
for f in sorted(p.parent.glob("*")):
    name = f.name
    cps = " ".join(hex(ord(c)) for c in name)
    print(repr(name), "->", cps, "| normalized:", ud.normalize("NFC", name))
